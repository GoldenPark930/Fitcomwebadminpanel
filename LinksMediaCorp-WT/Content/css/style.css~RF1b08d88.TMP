.error-msg{
    color:red;
    padding:5px 0 0 147px;
    float:left;
}
.error-msg.pad20 {
    padding-left: 20px;
}
.error-msg.pad75 {
    padding-left: 75px;
}
.error-msg.none{padding-left:0;}
.margin-top{margin-top:15px;}

.ui-datepicker-prev.ui-corner-all { float:left;
}
.ui-datepicker-next.ui-corner-all { float:right;
}
.ui-datepicker-calendar a { width: 23px;
height: 23px;
text-align: center;
display: block;
}
.ui-datepicker-calendar { padding:10px; display:block;
}
#ui-datepicker-div .ui-widget-header {background-color: #4b2b77;border:1px solid #4b2b77; background-image:none;
padding: 5px;
}
.date-time-picker-box{ width:400px; float:left; margin-bottom:15px;
}
    .date-time-picker-box span, .active-txt{ width:100px; float:left; display:block; padding-top: 10px;
line-height: 6px;
    }
.clr-both { clear:both;
}
.date-time-picker-box input { padding:4px; width:250px;
}
#page-wrapper .page-control-btn { width:192px; margin:0 auto; position:relative;
}
#page-wrapper .page-control-btn input, #page-wrapper .header-link-btn input{display: inline-block;
}
#page-wrapper .header-link-btn {width:192px; float:right;
}
.specialisation-txt {margin-right: 65px;
}
.txt-specialisation-detail { margin-bottom:15px;
}
.txt-specialisation-detail label{width: 180px; margin-left:5px;font-weight:normal;}
.txt-specialisation-detail legend { margin-bottom:5px;
}