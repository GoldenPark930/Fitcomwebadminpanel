                        @Html.ActionLink("Cancel", "Challenges", "Reporting", new { page = Model.Page, sort = Model.SortField, sortdir = Model.Sortdir }, new { @class = "submit" })
                    }
                </div>
            </div>
            <!-- end challenge-content -->
        </div>
        <!-- end create-challenge -->
        <div class="clearfix"></div>
    </div>
                                        }
