@model LinksMediaCorpEntity.CreateChallengeVM
<script src="~/Scripts/ckeditor/ckeditor.js"></script>
@using MvcCheckBoxList.Model
@using LinksMediaCorpUtility.Resources
@Scripts.Render("~/Content/stylesheet")
@Styles.Render("~/Content/croppie")
@Styles.Render("~/Content/challenge")
@Scripts.Render("~/bundles/ui")
@Scripts.Render("~/bundles/challenge")
@Scripts.Render("~/bundles/FeatureImageUpload")
@Scripts.Render("~/bundles/queue")

@{
    Layout = "~/Views/Shared/_FitComLayout.cshtml";
    ViewBag.Title = "EditChallenge";

    List<LinksMediaCorpEntity.ChallengeTypes> lstChallangeType = new List<LinksMediaCorpEntity.ChallengeTypes>();
    if (ViewBag.ChallengeTypeList != null)
    {
        lstChallangeType = ViewBag.ChallengeTypeList;
        lstChallangeType = lstChallangeType.GroupBy(e => new
        {
            empLoc = e.ChallengeType
        }).Select(g => g.FirstOrDefault()).ToList();
    }

    List<LinksMediaCorpEntity.ChallengeTypes> lstChallangeSubType = new List<LinksMediaCorpEntity.ChallengeTypes>();
    if (ViewBag.ChallengeTypeSubList != null)
    {
        lstChallangeSubType = ViewBag.ChallengeTypeSubList;
    }

    List<LinksMediaCorpEntity.TrainerViewVM> lstTrainers = new List<LinksMediaCorpEntity.TrainerViewVM>();
    if (ViewBag.Trainers != null)
    {
        lstTrainers = ViewBag.Trainers;
        lstTrainers = lstTrainers.GroupBy(e => new
        {
            trainerName = e.TrainerName
        }).Select(g => g.FirstOrDefault()).ToList();
    }

    List<LinksMediaCorpEntity.ExerciseType> lstExerciseType = new List<LinksMediaCorpEntity.ExerciseType>();
    if (ViewBag.ExerciseTypeList != null)
    {
        lstExerciseType = ViewBag.ExerciseTypeList;
    }

    List<LinksMediaCorpEntity.CreateUserVM> lstUsers = new List<LinksMediaCorpEntity.CreateUserVM>();
    if (ViewBag.Users != null)
    {
        lstUsers = ViewBag.Users;
        lstUsers = lstUsers.GroupBy(e => new
        {
            userName = e.UserName
        }).Select(g => g.FirstOrDefault()).ToList();
    }

    List<LinksMediaCorpEntity.UserResult> lstTrainerResult = new List<LinksMediaCorpEntity.UserResult>();
    if (ViewBag.TrainerResult != null)
    {
        lstTrainerResult = ViewBag.TrainerResult;
    }

    List<LinksMediaCorpEntity.UserResult> lstUserResult = new List<LinksMediaCorpEntity.UserResult>();
    if (ViewBag.UserResult != null)
    {
        lstUserResult = ViewBag.UserResult;
    }

    List<LinksMediaCorpEntity.ViewTrainers> lstallTrainers = new List<LinksMediaCorpEntity.ViewTrainers>();
    if (ViewBag.TrainerList != null)
    {
        lstallTrainers = ViewBag.TrainerList;
    }

    List<System.Web.WebPages.Html.SelectListItem> lstFractionList = new List<System.Web.WebPages.Html.SelectListItem>();
    if (ViewBag.FractionLIst != null)
    {
        lstFractionList = ViewBag.FractionLIst;
    }
    else
    {
        <script type="text/javascript">
            $(function () {
                $('#ResultFrection').hide();
            });
        </script>

    }

    /*Code for radio button list*/
    List<LinksMediaCorpEntity.BodyPart> lstBodyPart = new List<LinksMediaCorpEntity.BodyPart>();
    if (ViewBag.ListBodyPartList != null)
    {
        lstBodyPart = ViewBag.ListBodyPartList;
    }

    /*Code for radio button list of Equipment*/
    List<LinksMediaCorpEntity.Equipments> lstEquipment = new List<LinksMediaCorpEntity.Equipments>();
    if (ViewBag.ListEquipment != null)
    {
        lstEquipment = ViewBag.ListEquipment;
    }

    /*Code for radio button list of Equipment*/
    List<LinksMediaCorpEntity.Difficulties> lstDifficulty = new List<LinksMediaCorpEntity.Difficulties>();
    if (ViewBag.ListDifficulty != null)
    {
        lstDifficulty = ViewBag.ListDifficulty;
    }

    <script type="text/javascript">
        $('#Home_li').removeClass("active");
        $('#Reporing_li').removeClass('active');
        $('#chalenges_submenu').removeClass("inner-menu");
        $('#activities_submenu').removeClass('inner-menu');
        $('#trainers_submenu').removeClass("inner-menu");
        $('#users_submenu').removeClass('inner-menu');
        $('#teams_submenu').removeClass("inner-menu");
        $('#Reporing_li').addClass('active');

    </script>
    if (!Convert.ToString(@Session[LinksMediaCorpUtility.Resources.Message.PreviousUrl]).Equals(LinksMediaCorpUtility.Resources.Message.MainUrlText))
    {
        <script type="text/javascript">
            $('#chalenges_submenu').addClass("inner-menu");
        </script>
    }
}

<!-- start page-wrapper -->
@using (Html.BeginForm("UpdateChallenge", "Reporting", FormMethod.Post,
                                                  new { id = "myform", enctype = "multipart/form-data" }))
{
    <div id="page-wrapper">
        <!-- Start hidden field Section -->
        @Html.HiddenFor(m => m.VariableLimit)
        @Html.HiddenFor(model => model.ChallengeId)
        @Html.HiddenFor(model => model.CreatedBy)
        @Html.HiddenFor(model => model.CreatedDate)
        @Html.HiddenFor(m => m.IsMoreThenOne)
        @Html.HiddenFor(m => m.VariableUnit)
        @Html.HiddenFor(m => m.ResultUnitType)
        @Html.HiddenFor(m => m.ExeIndexLink1)
        @Html.HiddenFor(m => m.FreeFormExerciseNameDescriptionList)
        @Html.HiddenFor(m => m.SelectedAllIndex)
        @Html.HiddenFor(m => m.ExerciseId)
        @Html.HiddenFor(m => m.ExerciseId1)
        @Html.HiddenFor(m => m.FFExeDesc1)
        @Html.HiddenFor(m => m.SelectedChallengeTypeId)
        @Html.HiddenFor(model => model.SelectedEquipmentCheck)
        @Html.HiddenFor(m => m.Page)
        @Html.HiddenFor(m => m.SortField)
        @Html.HiddenFor(model => model.Sortdir)
        @Html.HiddenFor(model => model.CEARocordId1)
        @Html.HiddenFor(model => model.CropImageRowData)
        @Html.HiddenFor(model => model.FeaturedImageUrl)
        @Html.HiddenFor(model => model.SelectedChallengeCategoryCheck)
        <!-- End hidden field Section -->
        <!-- start create-challenge -->
        <div class="create-challenge">
            <h3>
                @Html.DisplayFor(m => m.ChallengeName)
                <div class="header-link-btn">
                    <input name="" type="button" id="btnTopAdminUpdateChallange" value="Update" class="submit" />
                    @if (Convert.ToString(@Session[LinksMediaCorpUtility.Resources.Message.PreviousUrl]).Equals(LinksMediaCorpUtility.Resources.Message.MainUrlText))
                    {
                        @Html.ActionLink("Cancel", "Main", "Reporting", new { }, new { @class = "submit" })
                    }
                    else
                    {
                        @Html.ActionLink("Cancel", "Challenges", "Reporting", new { page = Model.Page, sort = Model.SortField, sortdir = Model.Sortdir }, new { @class = "submit" })
                    }
                </div>
                <div class="clr-both"></div>
            </h3>
            <!-- start challenge-content -->
            <div class="challenge-content">
                <div class="step">
                    <h5>Manage Queue: </h5>
                    <!-- start step-des -->
                    <div class="step-des">
                        <div class="clr-both"></div>
                        <div class="row txt-lbl-input">
                            <div class="col-md-12 col-lg-12 col-xs-12">
                                <label><span class="active-new-chckbox pull-left">Active Challenge:</span>@Html.CheckBox("IsActive", new { @class = "checkbox-btn" })</label>
                                &nbsp; &nbsp; &nbsp; &nbsp;
                                <label id="divchkPremium"><span class="active-new-chckbox pull-left">Premium Challenge:</span>@Html.CheckBox("IsPremium", new { @class = "checkbox-btn" })</label>
                                &nbsp; &nbsp; &nbsp; &nbsp;
                                <label id="divchkFreeFitnessTest"><span class="active-new-chckbox pull-left">Free Fitness Tests:</span>@Html.CheckBox("IsFreeFitnessTest", new { @class = "checkbox-btn" })</label>
                                &nbsp; &nbsp; &nbsp; &nbsp;
                                <label id="divchkSubscription"><span class="active-new-chckbox pull-left">Subscription Challenge:</span>@Html.CheckBox("IsSubscription", new { @class = "checkbox-btn" })</label>
                                &nbsp; &nbsp; &nbsp; &nbsp;
                                <label id="divchkFeatured"><span class="active-new-chckbox pull-left">Featured Challenge:</span>@Html.CheckBox("IsFeatured", new { @class = "checkbox-btn" })</label>

                            </div>

                        </div>
                        <div class="clr-both"></div>
                        <div id="divqueue">
                            <div class="date-time-picker-box"><b>Set to COD Queue:</b> @Html.CheckBox("IsSetToCOD")</div>
                            <div id="divCOD" style="display:none;">
                                <div class="clr-both"></div>
                                <div class="row txt-lbl-input">
                                    <div class="col-md-6 col-lg-6 col-xs-6 ">
                                        <label>Start Date:</label>
                                        @Html.TextBoxFor(m => m.CODStartDate, "{0:MM/dd/yyyy}", new { @class = "input-text-right" })
                                        <div class="clr-both"></div>
                                        <div class="col-md-4 col-lg-4 col-xs-4"></div>
                                        <div class="col-md-6 col-lg-6 col-xs-6 remove-margin-left">
                                            @Html.ValidationMessageFor(m => m.CODStartDate, "", new { @class = "error-msg" })
                                        </div>
                                    </div>
                                    <div class="col-md-6 col-lg-6 col-xs-6 ">
                                        <label>End Date:</label>
                                        @Html.TextBoxFor(m => m.CODEndDate, "{0:MM/dd/yyyy}", new { @class = "input-text-right" })
                                        <div class="clr-both"></div>
                                        <div class="col-md-4 col-lg-4 col-xs-4"></div>
                                        <div class="col-md-6 col-lg-6 col-xs-6 remove-margin-left">
                                            @Html.ValidationMessageFor(m => m.CODEndDate, "", new { @class = "error-msg" })
                                        </div>
                                    </div>
                                </div>
                                <div class="clr-both"></div>
                                <div class="row txt-lbl-input">
                                    <div class="col-md-6 col-lg-6 col-xs-6 state-drop-down-listbox ">
                                        <label>User Full Name:</label>
                                        @Html.DropDownListFor(m => m.EndUserNameId, new SelectList(lstUsers, "UserId", "UserName"), "--Choose User--", new { @class = "select-drop" })
                                        <div class="clr-both"></div>
                                        <div class="col-md-4 col-lg-4 col-xs-4"></div>
                                        <div class="col-md-6 col-lg-6 col-xs-6 remove-margin-left">
                                            @Html.ValidationMessageFor(m => m.EndUserNameId, "", new { @class = "error-msg" })
                                        </div>
                                    </div>
                                    <div class="col-md-6 col-lg-6 col-xs-6 state-drop-down-listbox ">
                                        <label>Best Result:</label>
                                        <span id='loadingUserResult' style='display:none;'>
                                            <img src='~/images/ajax-loader.gif' />
                                        </span>
                                        @Html.DropDownListFor(m => m.UserResultId, new SelectList(lstUserResult, "Id", "Result"), "--Choose Result--", new { @class = "select-drop" })
                                        <div class="clr-both"></div>
                                        <div class="col-md-4 col-lg-4 col-xs-4"></div>
                                        <div class="col-md-6 col-lg-6 col-xs-6 remove-margin-left">
                                            @Html.ValidationMessageFor(m => m.UserResultId, "", new { @class = "error-msg" })
                                        </div>
                                    </div>
                                </div>
                                <div class="clr-both"></div>
                                <div class="row txt-lbl-input">
                                    <div class="col-md-6 col-lg-6 col-xs-6 ">
                                        <label>Video Link:</label>
                                        @Html.TextBoxFor(m => m.UserVideoLink, new { @class = "input-text-right", maxLength = "200" })
                                        <div class="clr-both"></div>
                                        <div class="col-md-4 col-lg-4 col-xs-4"></div>
                                        <div class="col-md-6 col-lg-6 col-xs-6 remove-margin-left">
                                            @Html.ValidationMessageFor(m => m.UserVideoLink, "", new { @class = "error-msg" })
                                        </div>
                                    </div>
                                </div>
                            </div><div class="clr-both"></div>
                            <div class="date-time-picker-box"><b>Set to Sponsor Challenge Queue:</b> @Html.CheckBox("IsSetToSponsorChallenge")</div>
                            <div id="divSC" style="display:none;">
                                <div class="clr-both"></div>
                                <div class="row txt-lbl-input">
                                    <div class="col-md-6 col-lg-6 col-xs-6 ">
                                        <label>Start Date:</label>
                                        @Html.TextBoxFor(m => m.TCStartDate, "{0:MM/dd/yyyy}", new { @class = "input-text-right" })
                                        <div class="clr-both"></div>
                                        <div class="col-md-4 col-lg-4 col-xs-4"></div>
                                        <div class="col-md-6 col-lg-6 col-xs-6 remove-margin-left">
                                            @Html.ValidationMessageFor(m => m.TCStartDate, "", new { @class = "error-msg" })
                                        </div>
                                    </div>
                                    <div class="col-md-6 col-lg-6 col-xs-6 ">
                                        <label>End Date:</label>
                                        @Html.TextBoxFor(m => m.TCEndDate, "{0:MM/dd/yyyy}", new { @class = "input-text-right" })
                                        <div class="clr-both"></div>
                                        <div class="col-md-4 col-lg-4 col-xs-4"></div>
                                        <div class="col-md-6 col-lg-6 col-xs-6 remove-margin-left">
                                            @Html.ValidationMessageFor(m => m.TCEndDate, "", new { @class = "error-msg" })
                                        </div>
                                    </div>
                                </div>
                                <div class="clr-both"></div>
                                <div class="row txt-lbl-input">
                                    <div class="col-md-6 col-lg-6 col-xs-6  state-drop-down-listbox">
                                        <label>Trainer Name:</label>
                                        @Html.DropDownListFor(m => m.TrainerId, new SelectList(lstTrainers, "TrainerId", "TrainerName"), "--Choose Trainer--", new { @class = "select-drop" })
                                        <div class="clr-both"></div>
                                        <div class="col-md-4 col-lg-4 col-xs-4"></div>
                                        <div class="col-md-6 col-lg-6 col-xs-6 remove-margin-left">
                                            @Html.ValidationMessageFor(m => m.TrainerId, "", new { @class = "error-msg" })
                                        </div>
                                    </div>
                                    <div class="col-md-6 col-lg-6 col-xs-6  state-drop-down-listbox">
                                        <label>Best Result:</label>
                                        <span id='loadingTrainerResult' style='display:none;'>
                                            <img src='~/images/ajax-loader.gif' />
                                        </span>
                                        @Html.DropDownListFor(m => m.ResultId, new SelectList(lstTrainerResult, "Id", "Result"), "--Choose Result--", new { @class = "select-drop" })
                                        <div class="clr-both"></div>
                                        <div class="col-md-4 col-lg-4 col-xs-4"></div>
                                        <div class="col-md-6 col-lg-6 col-xs-6 remove-margin-left">
                                            @Html.ValidationMessageFor(m => m.ResultId, "", new { @class = "error-msg" })
                                        </div>
                                    </div>
                                </div>
                                <div class="clr-both"></div>
                                <div class="row txt-lbl-input">
                                    <div class="col-md-6 col-lg-6 col-xs-6 ">
                                        <label>Video Link:</label>
                                        @Html.TextBoxFor(m => m.TrainerVideoLink, new { @class = "input-text-right", maxLength = "200" })
                                        <div class="clr-both"></div>
                                        <div class="col-md-4 col-lg-4 col-xs-4"></div>
                                        <div class="col-md-6 col-lg-6 col-xs-6 remove-margin-left">
                                            @Html.ValidationMessageFor(m => m.TrainerVideoLink, "", new { @class = "error-msg" })
                                        </div>
                                    </div>
                                    <div class="col-md-6 col-lg-6 col-xs-6 ">
                                        <label>Sponsor Name:</label>
                                        @Html.TextBoxFor(m => m.SponsorName, null, new { @class = "input-text-right", maxLength = "100" })
                                        <div class="clr-both"></div>
                                        <div class="col-md-4 col-lg-4 col-xs-4"></div>
                                        <div class="col-md-6 col-lg-6 col-xs-6 remove-margin-left">
                                            @Html.ValidationMessageFor(m => m.SponsorName, "", new { @class = "error-msg" })
                                        </div>
                                    </div>
                                </div>
                                <div class="clr-both"></div>
                            </div>
                        </div>
                        <div class="clearfix"></div>



                        <div id="FeaturedUploadSection" class="row txt-lbl-input">
                            <div class="col-md-2 col-lg-2 col-xs-2 mob-view-box">
                                <label>Featured Photo :</label>
                            </div>
                            <div class="col-md-10 col-lg-10 col-xs-10 mob-view-box">
                                <div class="fileupload fileupload-new col-md-10">
                                    <div id="existingFeaturedImgConatiner" class="fileupload-new thumbnail" style="max-height:260px;max-width:320px;">
                                        <img id="existingFeaturedImg" src='@ViewBag.FeaturedPhoto' alt="Photo" />
                                    </div>
                                </div>
                                <div class="clearfix"></div>
                                <div class="upload-FeaturedImage">
                                    <div class="grid">

                                        <div class="col-md-10 select-img-btn">
                                            <div class="fileupload-preview thumbnail" id="upload-FeaturedImage"></div>
                                        </div>
                                    </div>
                                </div>
                                <div class="actions">
                                    <div class="col-md-10 ation-btn-container">
                                        <a class="btn btn-primary file-btn">
                                            <span class="fileupload-new">Select Image</span>
                                            <input type="file" id="FeaturedUpload" value="Choose a file" accept="image/*" />
                                        </a>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- end step-des -->
                </div>
                <!-- start step -->
                <div class="step">
                    <h5>Select Challenge Type: </h5>
                    <!-- start step-des -->
                    <div class="step-des">
                        @Html.DropDownListFor(m => m.ChallengeType, new SelectList(lstChallangeType, "ChallengeSubTypeId", "ChallengeType", Model.SelectedChallengeTypeId), "--Choose Challenge Type--", new { @class = "select", onchange = "onChangeId();" })
                        <div class="clearfix"></div>
                        @Html.ValidationMessageFor(m => m.ChallengeType, "", new { @class = "error-msg pad0" })
                    </div>
                    <div class="step-des">
                        @Html.DropDownListFor(m => m.ChallengeSubTypeId, new SelectList(lstChallangeSubType, "ChallengeSubTypeId", "ChallengeSubType", Model.ChallengeSubTypeId), "--Choose Challenge Sub-Type--", new { onchange = "onChangeId();", @class = "select" })
                        <span id='loadingChallengeSubType' style='display:none;'>
                            <img src='~/images/ajax-loader.gif' />
                        </span>
                        <div class="clearfix"></div>
                        @Html.ValidationMessageFor(m => m.ChallengeSubTypeId, "", new { @class = "error-msg pad0" })
                    </div>
                    <!-- end step-des -->
                </div>
                <div class="step" id="divChallengeCategorySection">
                    <h5>Select Your Category: </h5>
                    <!-- start step-des -->
                    <div class="col-xs-12 col-md-12 col-lg-12 mob-view-box">
                        <div style="overflow-x:auto; overflow-y:hidden;">
                            <fieldset class="txt-specialisation-detail margin-top" style="width:96%;">
                                <div id="chkAllChallengeCategory">
                                    <div style="color:red;"> @Html.ValidationMessageFor(m => m.SelectedChallengeCategoryCheck, "", new { })</div>
                                    @{
                                        var primaryChallengeCategoryListInfo = new HtmlListInfo(HtmlTag.table, 5, null, TextLayout.Default, TemplateIsUsed.No);
                                        @Html.CheckBoxListFor(m => m.PostedChallengeCategory.ChallengeCategoryId, m => m.AvailableChallengeCategory,
m => m.ChallengeCategoryId, m => m.ChallengeCategoryName, m => m.SelecetdChallengeCategory, primaryChallengeCategoryListInfo)
                                    }
                                </div>
                            </fieldset>
                        </div>
                    </div>
                    <!-- end step-des -->
                </div>

                <!-- end step -->
                <!-- start step -->
                <div class="step" id="CreateChangeDifficultyLevel">
                    <h5>Select Difficulty Level:</h5>
                    <!-- start step-des -->
                    <div class="step-des">

                        @{
                            int i = 0;
                            foreach (var item in lstDifficulty)
                            {
                                <div class="seq-input-lbl">
                                    @if (i == 0)
                                    {
                                        @Html.RadioButtonFor(m => m.DifficultyLevel, item.Difficulty, new { @class = "radio", @checked = true })
                                        i = i + 1;
                                    }
                                    else
                                    {
                                        @Html.RadioButtonFor(m => m.DifficultyLevel, item.Difficulty, new { @class = "radio" })
                                    }
                                    @Html.LabelFor(m => m.DifficultyLevel, item.Difficulty, new { @class = "label" })
                                </div>
                            }
                        }


                    </div>
                    <!-- end step-des -->
                </div>
                <!-- end step -->
                <div id="FreeFormChallegeDescriptionHeaderSection" class="step">
                    <h5 id="ChallegeDescriptionHeader">Describe Your Challenge: </h5>
                    <!-- start step-des -->
                    <div class="row txt-lbl-input">
                        <div class="col-md-12 col-lg-12 col-xs-12 mob-view-box">
                            <div class="clr-both"></div>
                            <div class="col-md-12 col-lg-12 col-xs-12"></div>
                            <div class="col-md-6 col-lg-6 col-xs-6 remove-margin-left">  @Html.ValidationMessageFor(m => m.Description, "", new { @class = "error-msg" })</div>
                        </div>
                        <div class="col-md-12 col-lg-12 col-xs-12 mob-view-box">
                            @Html.TextAreaFor(m => m.Description, new { @class = "ckeditor", maxLength = "1000", placeholder = "Max Char limit 1000" })
                        </div>
                    </div>

                    <!-- end step-des -->
                </div>
                <div id="FreeFormChallegeDetailsSection" class="step">
                    <h5 id="ChallegeDescriptionHeader">Challenge Details:</h5>
                    <!-- start step-des -->
                    <div class="row txt-lbl-input">
                        <div class="col-md-12 col-lg-12 col-xs-12 mob-view-box">
                            <div class="clr-both"></div>
                            <div class="col-md-12 col-lg-12 col-xs-12"></div>
                            <div class="col-md-6 col-lg-6 col-xs-6 remove-margin-left">  @Html.ValidationMessageFor(m => m.ChallengeDetail, "", new { id = "spnChallengeDetails", @class = "error-msg" })</div>
                        </div>
                        <div class="col-md-12 col-lg-12 col-xs-12 mob-view-box">
                            @Html.TextAreaFor(m => m.ChallengeDetail, new { @class = "ckeditor", maxLength = "1000", placeholder = "Max Char limit 1000" })
                        </div>
                    </div>
                    <!-- end step-des -->
                </div>
                <div class="step" id="FreeFormChallegeDurationSection">
                    <h5>How long is Workout:</h5>
                    <!-- start step-des -->
                    <div class="step-des">
                        <div class="row-colum">
                            @Html.TextBoxFor(m => m.FFChallengeDuration, null, new { @class = "input", maxLength = "200" })
                            @Html.ValidationMessageFor(m => m.FFChallengeDuration, "", new { @class = "error-msg none error-btm" })
                        </div>
                    </div>
                    <!-- end step-des -->
                </div>
                <!-- start step -->
                <div class="step" id="FitcomChallengeExeciseSection">
                    <h5>Choose Exercise(s):</h5>
                    <!-- start step-des -->
                    <div class="step-des">
                        <div class="row-colum">
                            <div>
                                <label class="pull-left" style="width: auto; margin-right: 5px; margin-left: 50px; padding: 5px 1%; line-height: 25px;">Equipments:</label>
                                @*<select id="chkSearchEquipment" multiple="multiple" title="Equipment" style="width:180px;"></select>*@
                                @Html.DropDownListFor(m => m.SelectedFitcomEquipment1, new SelectList(lstEquipment, "EquipmentId", "Equipment"), "--Choose Equipments--", new { @class = "select" })

                            </div>
                            <div>
                                <label class="pull-left" style="width: auto; margin-right: 5px; margin-left: 10px; padding: 5px 1%; line-height: 25px;">Training Zones:</label>
                                @*<select id="chkTrainingZone" multiple="multiple" title="Training Zone" style="width:180px;"></select>*@
                                @Html.DropDownListFor(m => m.SelectedFitcomTrainingZone1, new SelectList(lstBodyPart, "PartId", "PartName"), "--Choose Training Zones--", new { @class = "select" })
                            </div>
                            <div>
                                <label class="pull-left" style="width: auto; margin-right: 5px; margin-left: 10px; padding: 5px 1%; line-height: 25px;">Exercise Types:</label>
                                @*<select id="chkSearchExerciseType" multiple="multiple" title="Exercise Type" style="width:180px;"></select>*@
